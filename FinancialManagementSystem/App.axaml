<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="FinancialManagementSystem.App"
             xmlns:local="using:FinancialManagementSystem"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <StyleInclude Source="avares://Semi.Avalonia/Themes/Index.axaml" />
        <StyleInclude Source="avares://FinancialManagementSystem/Assets/Icons.axaml" />
    </Application.Styles>
             
    <Application.Resources>
        <FontFamily x:Key="MontserratFont">avares://FinancialManagementSystem/Assets/Fonts#Montserrat</FontFamily>
    </Application.Resources>
             
</Application>